<?xml version="1.0"?>
<config>
    <modules>
        <QM_QMExtPdfDocs>
            <version>0.0.1</version>
        </QM_QMExtPdfDocs>
    </modules>
    <global>
        <models>
            <qmextpdf>
                <class>QM_QMExtPdfDocs_Model</class>
            </qmextpdf>
            <sales>
                <rewrite>
                    <order_pdf_invoice>QM_QMExtPdfDocs_Model_Order_Pdf_Invoice</order_pdf_invoice>
                    <order_pdf_shipment>QM_QMExtPdfDocs_Model_Order_Pdf_Shipment</order_pdf_shipment>
                    <order_pdf_items_invoice_default>QM_QMExtPdfDocs_Model_Order_Pdf_Items_Invoice_InvoiceItem</order_pdf_items_invoice_default>
                    <order_pdf_items_shipment_default>QM_QMExtPdfDocs_Model_Order_Pdf_Items_Invoice_ShipmentItem</order_pdf_items_shipment_default>
                </rewrite>
            </sales>
        </models>
        <helpers>
            <qmExtPdfDocs>
                <class>QM_QMExtPdfDocs_Helper</class>
            </qmExtPdfDocs>
        </helpers>
        <blocks>
            <qm_pdf>
                <class>QM_QMExtPdfDocs_Block</class>
            </qm_pdf>
        </blocks>
    </global>
    <frontend>
        <routers>
            <qmcore>
                <use>standard</use>
                <args>
                    <module>QM_QMExtPdfDocs</module>
                    <frontName>print</frontName>
                </args>
            </qmcore>
        </routers>
    </frontend>
</config>