<?xml version="1.0"?>
<layout version="0.1.0">
    <checkout_cart_index>
        <reference name="head">
            <action method="addItem">
                <type>skin_js</type>
                <name>js/qm_newpostv2.js</name>
            </action>
            <action method="addItem">
                <type>skin_css</type>
                <name>css/qm_newpostv2/qm_newpostv2.css</name>
            </action>
            <action method="addItem">
                <type>skin_css</type>
                <name>css/qm_newpostv2/chosen.css</name>
            </action>
        </reference>

        <reference name="before_body_end">
            <block type="qm_newpostv2/checkout_onepage_optionsWarehouseWarehouse" template="qm/newpostv2/checkout/onepage/formWarehouseWarehouse.phtml"/>
            <block type="qm_newpostv2/checkout_onepage_optionsWarehouseDoors" template="qm/newpostv2/checkout/onepage/formWarehouseDoors.phtml"/>
        </reference>

        <update handle="additional_shipping_available_data_update"/>
    </checkout_cart_index>
</layout>